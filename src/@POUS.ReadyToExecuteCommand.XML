								<BOOL/>
							</type>
						</variable>
						<variable name="LX_ScsValuePrev" group="Internal">
							<type>
								<BOOL/>
							</type>
						</variable>
					</localVars>
					<inOutVars retain="false">
						<variable name="INOUT_LX_ScsValue" group="Inouts">
							<type>
